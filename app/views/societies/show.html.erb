<h1><%= @society.name %></h1>



<ul class="list-unstyled">
  <% @championships.each do |championship| %>
    <li data-bbq="<%= championship.id %>">
      <h2>
        <a href="<%= society_championship_path(id:championship.id, society_id: @society.id) %>">
          <%= championship.name  + " " + championship.kind %>
        </a>
      </h2>
    </li>
  <% end %>
</ul>
